{
  "env": {
    "test": {
      "presets": [["env", {"targets": {"node": "current"}}], "react"],
      "plugins": ["transform-object-rest-spread"]
    },
    "lib": {
      "presets": [["env"], "react"],
      "plugins": ["transform-object-rest-spread"]
    },
    "rollup-lib": {
      "presets": [["env", {"loose": true, "modules": false}], "react"],
      "plugins": ["external-helpers", "transform-object-rest-spread"]
    },
    "rollup-browser": {
      "presets": [["env", {"loose": true, "modules": false, "targets": {"browsers": ["last 2 versions", "ie 11"]}}], "react"],
      "plugins": ["external-helpers", "transform-object-rest-spread"]
    }
  }
}
